{
    "$id": "past_match.schema.json",
    "type": "object",
    "required": ["id", "users", "ended_at", "outcome"],
    "properties": {
      "id": { "$ref": "../common/uuid.schema.json" },
      "users": {
        "type": "array",
        "items": { "$ref": "../common/uuid.schema.json" },
        "minItems": 2,
        "maxItems": 2
      },
      "ended_at": { "$ref": "../common/timestamp.schema.json" },
      "outcome": {
        "type": "string",
        "enum": ["mutual_stop", "blocked", "expired"]
      }
    }
  }
  